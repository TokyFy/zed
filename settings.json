{
	"auto_update_extensions": {
  "make": false
 },
 "theme": "Catppuccin Latte",
 "assistant": {
		"default_model": {
			"provider": "copilot_chat",
			"model": "gpt-4o"
		},
		"version": "2"
	},
	"buffer_font_family": "JetBrains Mono",
	"buffer_font_weight": 600,
	"ui_font_family": "JetBrains Mono",
	"hard_tabs": true,
	"vim_mode": true,
	"ui_font_size": 15,
	"buffer_font_size": 16
}
